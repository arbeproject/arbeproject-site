---
import "../styles/tailwind.css";

const { title = "ARBE Project", description = "" } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>{title}</title>
    <meta name="description" content={description} />
  </head>
  <body class="arbe-page">
    <main class="arbe-container py-8 md:py-12">
      <slot />
    </main>
  </body>
</html>







